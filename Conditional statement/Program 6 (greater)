#include <stdio.h>
int main()
{
    int num1,num2,num3;
    printf("Enter num1,num2,num3:");
    scanf("%d%d%d",&num1,&num2,&num3);
    if(num2>num1)
    {
        if(num3>num2)
        {
            printf("%d is greater number",num3);
        }
        else
        {
            printf("%d is greater number",num2);
        }
    }
    else
        {
           printf("%d is greater number",num1);
        }
    return 0;
}
